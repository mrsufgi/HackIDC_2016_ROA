{
	"extends": [
		"standard",
		"plugin:react/recommended"
	],
	"parser": "babel-eslint",
	"ecmaFeatures": {
		"jsx": true
	},
	"plugins": [
		"babel",
		"react"
	],
	"rules": {
		"indent": [
			"error",
			"tab",
			{
				"SwitchCase": 1
			}
		],
		"semi": ["error", "always"],
		"space-before-function-paren": ["error", "never"]
	}
}